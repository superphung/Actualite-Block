{
  "name": "ActualitéBlock",
  "description": "block limitation view of lexpress",
  "version": "2.0",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["http://lexpansion.lexpress.fr/*"],
      "js": ["lexpress.js"]
    },
    {
      "matches": ["http://*.leparisien.fr/*"],
      "js": ["leparisien.js"]
    },
    {
      "matches": ["http://*.lefigaro.fr/*"],
      "js": ["lefigaro.js"]
    },
    {
      "matches": ["http://*.lequipe.fr/*"],
      "js": ["lequipe.js"]
    },
    {
      "matches": ["http://*.europe1.fr/*"],
      "js": ["europe.js"]
    },
    {
      "matches": ["http://*.parismatch.com/*"],
      "js": ["europe.js"]
    }
  ],
  "browser_action": {
    "default_icon": "news.png",
    "default_title": "ActualitéBlock"
  },
  "manifest_version": 2
}
